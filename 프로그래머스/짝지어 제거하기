def solution(s):
    answer = 0
    while s == "":
        for i in range(len(s)-1):
            if s[i] == s[i+1]:
                s.replace(s*2, "")
                print(s)
                break
    if s == "":
        answer = 1
    return answer

print(solution("baabaa"))
print(solution("cdcd"))